<div class="row">
  <%@users.each do |user|%>
  <div class="col-md-4">
    <div class="well">
      <h4><%=user.username%></h4>
      <p class="pull-right">
        Joined on <%=user.created_at.strftime('%b %d %y')%>
</p>
<h4>
<%= link_to user.username, show_user_path(id: user.id)%>
</h4>

<p>
  <strong>Bio:</strong><%= user.bio%>
</p>
    </div>

  </div>
<%end%>
</div>
